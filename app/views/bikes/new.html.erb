<%= form_for @bike do |f| %>
	<div id="form-group">
		<%= f.label :brand %>
		<%= f.text_field :brand %>
	</div>
	<div id="form-group">
		<%= f.label :model %>
		<%= f.text_field :model %>
	</div>
	<div id="form-group">
		<%= f.label :type %>
		<%= f.text_field :type %>
	</div>
	<div id="form-group">
		<%= f.label :description %>
		<%= f.text_field :description %>
	</div>
	<div id="form-group">
		<%= f.label :year_made %>
		<%= f.text_field :year_made %>
	</div>
	<div id="form-group">
		<%= f.submit %>
	</div>
<% end %>

